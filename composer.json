{
  "name": "lonecat/psr7",
  "description": "My PSR-7 implementation",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Alex Melentev",
      "email": "alex@melentev.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "LoneCat\\PSR7\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LoneCat\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=7.4",
    "ext-mbstring": "*",
    "psr/http-message": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}